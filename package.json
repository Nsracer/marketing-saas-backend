{
  "name": "seo-health-score-api",
  "version": "1.0.0",
  "description": "SEO Health Score API using Lighthouse and Google Analytics",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:optimized": "node --max-old-space-size=512 --expose-gc --optimize-for-size --gc-interval=100 server.js",
    "dev": "nodemon server.js",
    "dev:optimized": "nodemon --max-old-space-size=512 --expose-gc server.js",
    "build": "./build.sh",
    "postinstall": "npx @puppeteer/browsers install chrome",
    "test:puppeteer": "node test-puppeteer-setup.js",
    "test:fallback": "node test-fallback.js",
    "test:stripe": "node test-stripe.js",
    "test:stripe-manual": "node test-stripe-manual.js"
  },
  "dependencies": {
    "@google-analytics/data": "^5.2.0",
    "@google/generative-ai": "^0.24.1",
    "@sparticuz/chromium": "^131.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "apify-client": "^2.19.0",
    "axios": "^1.12.2",
    "chrome-launcher": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jsdom": "^27.0.0",
    "jspdf": "^3.0.3",
    "lighthouse": "^11.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.28.0",
    "pdfkit": "^0.15.2",
    "pg": "^8.16.3",
    "puppeteer": "^24.23.0",
    "stripe": "^20.2.0"
  },
  "devDependencies": {
    "@types/jspdf": "^1.3.3",
    "nodemon": "^3.0.1"
  }
}
