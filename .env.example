# Server Configuration
PORT=3010
NODE_ENV=development

# Frontend/Backend URLs
# Development:
FRONTEND_URL=http://localhost:3002
BACKEND_URL=http://localhost:3010
# Production (Render):
# FRONTEND_URL=https://saas-frontend-o2cx.onrender.com
# BACKEND_URL=https://saas-wets.onrender.com

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Development:
GOOGLE_REDIRECT_URI=http://localhost:3010/api/auth/google/callback
# Production (Render):
# GOOGLE_REDIRECT_URI=https://saas-wets.onrender.com/api/auth/google/callback

# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_ANALYTICS_PROPERTY_ID=your_ga4_property_id
GOOGLE_SEARCH_CONSOLE_SITE_URL=https://yourdomain.com/
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json
GOOGLE_CLOUD_PROJECT_ID=your_project_id

# Supabase Configuration (for backend database access)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key

# Third-party API Keys
SE_RANKING_API_TOKEN=your_se_ranking_token
SE_RANKING_BASE_URL=https://api.seranking.com/
GEMINI_API_KEY=your_gemini_api_key
RAPIDAPI_KEY=your_rapidapi_key
SEARCHAPI_KEY=your_searchapi_key
CHANGEDETECTION_API_KEY=your_changedetection_key
SIMILARWEB_API_KEY=your_similarweb_key

# Facebook OAuth
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret

# Instagram OAuth (uses same Facebook App)
INSTAGRAM_APP_ID=your_instagram_app_id
INSTAGRAM_APP_SECRET=your_instagram_app_secret

# LinkedIn OAuth
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
# Development:
LINKEDIN_REDIRECT_URI=http://localhost:3002/auth/linkedin/callback
# Production (Render):
# LINKEDIN_REDIRECT_URI=https://saas-frontend-o2cx.onrender.com/auth/linkedin/callback

# Apify (for LinkedIn, Instagram, Facebook scraping)
APIFY_API_KEY=your_apify_api_key
APIFY_API_TOKEN=your_apify_api_token

# External Puppeteer API Configuration
# IMPORTANT: Use the PRODUCTION URL, not preview deployments
# Production URL format: https://your-project-name.vercel.app/api/analyze
PUPPETEER_API_URL=https://puppeteer-on-vercel-red.vercel.app/api/analyze
# Optional: API key if authentication is required
PUPPETEER_API_KEY=